{{#each items}}
	<ul class="collapsibleList" id={{collectionId}}> <span class="data-label"><b>{{collectionName}}</b></span> <br/>
	{{#each signals}}
		<li name={{fieldName}} display_name="{{displayName}}" type={{../classType}} class = "{{item_class}} tooltip"><span>{{displayName}}</span><span class="tooltiptext tooltip-top">{{{help_text}}}</span></li>
	{{/each}}
	</ul>
{{/each}}

